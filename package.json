    "eslint": "^8.34.0",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
